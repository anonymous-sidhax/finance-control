

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"

}

datasource db {
  provider = "sqlite"
 
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  updatedAt  DateTime @updatedAt
  createdAt DateTime @default(now())
}

model Category {
  id          Int      @id @default(autoincrement())
  name        String
  description String?
  updatedAt   DateTime  @updatedAt
  createdAt   DateTime @default(now())
}
